{
  "name": "hobnob",
  "version": "0.1.0",
  "description": "hobnob",
  "main": "index.js",
  "scripts": {
    "watch": "nodemon -w src --exec yarn run serve",
    "build": " rimraf dist/ && npx babel src -d dist",
    "serve": "yarn run build && dotenv node dist/index.js",
    "fix": "eslint src --fix",
    "test:e2e": "dotenv cucumber-js -- spec/cucumber/features --require-module @babel/register --require spec/cucumber/steps",
    "lint": "eslint src"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nonownonow/hobnob.git"
  },
  "keywords": [
    "hobnob"
  ],
  "author": "ilhwan ryu <ih.ryu.dev@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/nonownonow/hobnob/issues"
  },
  "homepage": "https://github.com/nonownonow/hobnob#readme",
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.4.0",
    "@babel/node": "^7.2.2",
    "@babel/preset-env": "^7.4.2",
    "cucumber": "4.2.1",
    "dotenv-cli": "^2.0.0",
    "eslint": "^5.15.3",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.12.4",
    "husky": "^1.3.1",
    "nodemon": "^1.18.10",
    "rimraf": "^2.6.3",
    "superagent": "^5.0.2"
  },
  "dependencies": {
    "@babel/polyfill": "^7.4.0",
    "body-parser": "^1.18.3",
    "dotenv": "^7.0.0",
    "express": "^4.16.4"
  },
  "eslintIgnore": [
    "dist",
    "node_modules"
  ]
}
